@model Service
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/AdminArea/Views/Shared/_AdminLayout.cshtml";
}




<div class="container my-5">
    <form class="forms-sample mt-5" method="post" asp-action="Create" enctype="multipart/form-data">
       
        <div class="form-group">
            <label asp-for="Header">Header:</label>
            <input type="text" class="form-control" style="width:300px" asp-for="Header">
            <span class="text-danger" asp-validation-for="Header"></span>
        </div>
        <div class="form-group">
            <label asp-for="Desc">Description:</label>
            <input type="text" class="form-control" style="width:300px" asp-for="Desc">
            <span class="text-danger" asp-validation-for="Desc"></span>
        </div>
        
        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>
</div>

